# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def share_pods
  pod 'RxSwift', '~> 6.9'
  pod 'RxCocoa', '~> 6.9'
  pod 'SnapKit'
  pod 'Kingfisher', '~> 7.0'
  pod 'Alamofire'
  pod 'SwiftSoup'
  pod "Solar-dev", "~> 3.0"
end


target 'QWERTY_Mini' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  share_pods
  # Pods for QWERTY_Mini

end

target 'QWERTY_Keyboard' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  share_pods
  # Pods for QWERTY_Mini

end
